
function response(nums, target){
    console.log(nums);
    for(let i = 0; i < nums.length; i++){
        for(let j = i + 1; j< nums.length; j++){
            if(nums[i] + nums[j] === target){
                return [nums[i], nums[j]];
        }
    }
}
}
var result = response([1,2,3,4], 6);
console.log(result)
